#include "lists.h"

/**
 * print_list - prints all the elements of a list_t list
 * @h: pointer to a list_t
 * Return: the number of nodes
 */
size_t print_list(const list_t *h)
{
	size_t numofnodes;

	numofnodes = 0;

	while (h != NULL)
	{
		printf("[%d] %s\n", h->len, h->str);
		numofnodes++;
	}
	return (numofnodes);
}
